{{#hvisTekstFinnes (concat key ".sporsmal")}}
    {{#hvisTekstFinnes (concat key ".undertittel")}}
        <h{{{niva}}}>{{{ hentTekst (concat key ".undertittel") }}}</h{{{niva}}}>
        {{#variabel "niva" (inc niva)}}
            <h{{niva}}>{{{ hentTekst (concat key ".sporsmal") }}}</h{{niva}}>
        {{/variabel}}
    {{else}}
        <h{{niva}}>{{{ hentTekst (concat key ".sporsmal") }}}</h{{niva}}>
    {{/hvisTekstFinnes}}
{{/hvisTekstFinnes}}