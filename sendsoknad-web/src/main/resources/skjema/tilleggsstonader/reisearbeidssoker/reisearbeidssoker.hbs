{{#forFaktumHvisSant "informasjonsside.stonad.reise"}}
    {{#forFaktumHvisSant "informasjonsside.stonad.reisearbeidssoker"}}
        {{#hvisSant value}}
            <h2>{{hentTekst "soknad.reise.arbeidssoker.tittel"}}</h2>
            <table>
                {{> skjema/tilleggsstonader/reisearbeidssoker/registrert}}
                {{> skjema/tilleggsstonader/reisearbeidssoker/hvorforreise}}
                {{> skjema/tilleggsstonader/reisearbeidssoker/dekketavandre}}
                {{> skjema/tilleggsstonader/reisearbeidssoker/dagpenger}}
                {{> skjema/tilleggsstonader/reisearbeidssoker/avstand}}

                {{#variabel "faktumKey" "reise.arbeidssoker.reisemaal"}}
                    {{> skjema/tilleggsstonader/felles/reisemaladresse}}
                {{/variabel}}

                {{#variabel "faktumPrefiks" "reise.arbeidssoker."}}
                    {{> skjema/tilleggsstonader/felles/offentligtransport}}
                {{/variabel}}
            </table>
        {{/hvisSant}}
    {{/forFaktumHvisSant}}
{{/forFaktumHvisSant}}
