{{#forFaktum "reise.arbeidssoker.dagpenger"}}
    <tr>
        <td class="sjekket"></td>
        <td>{{hentTekst (concat "soknad.reise.arbeidssoker.dagpenger." value) }}</td>
    </tr>
    {{#hvisLik value "true"}}
        <tr>
            <td></td>
            <td>
                <table>
                    {{#forFaktum "reise.arbeidssoker.dagpenger.forlenget"}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst (concat "soknad.reise.arbeidssoker.dagpenger.forlenget." value ".label") }}</td>
                        </tr>
                    {{/forFaktum}}
                    {{#forFaktum "reise.arbeidssoker.dagpenger.bortfall"}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst (concat "soknad.reise.arbeidssoker.dagpenger.bortfall." value ".label")}}</td>
                        </tr>
                    {{/forFaktum}}
                </table>
            </td>
        </tr>
    {{/hvisLik}}
{{/forFaktum}}
