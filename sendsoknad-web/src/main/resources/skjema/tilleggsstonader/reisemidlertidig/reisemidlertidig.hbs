{{#forFaktumHvisSant "informasjonsside.stonad.reise"}}
    {{#forFaktumHvisSant "informasjonsside.stonad.reisemidlertidig"}}
        <h2>{{hentTekst "soknad.reise.midlertidig.tittel"}}</h2>
        <table>
            {{#forFaktum "reise.midlertidig.periode"}}
                {{> skjema/tilleggsstonader/felles/aktivitetsperiode}}
            {{/forFaktum}}
            {{> skjema/tilleggsstonader/reisemidlertidig/avstand}}
            {{> skjema/tilleggsstonader/reisemidlertidig/antall}}

            {{#variabel "faktumKey" "reise.midlertidig.reisemaal"}}
                {{> skjema/tilleggsstonader/felles/reisemaladresse}}
            {{/variabel}}

            {{> skjema/tilleggsstonader/reisemidlertidig/barnsomflyttermed}}
            {{> skjema/tilleggsstonader/reisemidlertidig/hjemmeboende}}
            {{> skjema/tilleggsstonader/reisemidlertidig/sarskiltebehov}}
            {{#variabel "faktumPrefiks" "reise.midlertidig."}}
                {{> skjema/tilleggsstonader/felles/offentligtransport}}
            {{/variabel}}
        </table>
    {{/forFaktumHvisSant}}
{{/forFaktumHvisSant}}
