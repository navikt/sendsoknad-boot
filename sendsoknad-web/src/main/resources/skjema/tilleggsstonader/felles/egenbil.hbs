{{#forFaktumHvisSant (concat faktumPrefiks "offentligtransport.egenbil")}}
    <tr>
        <td class="sjekket"></td>
        <td>
            <span>
                {{hentTekst "soknad.reise.offentligtransport.egenbil.true"}}.
                {{hentTekst "oppsummering.reise.offentligtransport.egenbil.kostnader"}}
            </span>
        </td>
    </tr>

    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader")}}
        <tr>
            <td></td>
            <td>
                <table>
                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader.bompenger")}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.kostnader.bompenger.label"}}: <em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                        </tr>
                    {{/forFaktum}}
                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader.parkering")}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.kostnader.parkering.label"}}: <em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                        </tr>
                    {{/forFaktum}}
                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader.piggdekk")}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.kostnader.piggdekk.label"}}: <em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                        </tr>
                    {{/forFaktum}}
                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader.ferge")}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.kostnader.ferge.label"}}: <em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                        </tr>
                    {{/forFaktum}}
                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.kostnader.annet")}}
                        <tr>
                            <td class="sjekket"></td>
                            <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.kostnader.annet.label"}}: <em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                        </tr>
                    {{/forFaktum}}
                </table>
            </td>
        </tr>
    {{/forFaktum}}

    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.parkering")}}
        <tr>
            <td></td>
            <td>
                <table>
                    {{#hvisSant value}}
                        <tr>
                            <td class="sjekket"></td>
                            <td><span>{{hentTekst "soknad.reise.offentligtransport.egenbil.parkering.true"}}</span></td>
                        </tr>

                        <tr>
                            <td></td>
                            <td>
                                <table>
                                    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.parkering.belop")}}
                                        <tr>
                                            <td class="sjekket"></td>
                                            <td><em>{{value}}</em> {{hentTekst "soknad.krdag"}}</td>
                                        </tr>
                                    {{/forFaktum}}
                                </table>
                            </td>
                        </tr>
                    {{else}}
                        <tr>
                            <td class="sjekket"></td>
                            <td><span>{{hentTekst "soknad.reise.offentligtransport.egenbil.parkering.false"}}</span>
                            </td>
                        </tr>
                    {{/hvisSant}}
                </table>
            </td>
        </tr>
    {{/forFaktum}}

    {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.sendebekreftelse")}}
        <tr>
            <td></td>
            <td>
                <table>
                    <tr>
                        <td class="sjekket"></td>
                        <td>
                            {{hentTekst "soknadtilleggsstonader.oppsummering.dagligreise.kjorelisteperiode"}}
                            {{#hvisLik value "uke"}}
                                {{hentTekst "soknad.reise.offentligtransport.egenbil.sendebekreftelse.uke"}}
                            {{else}}
                                {{hentTekst "soknad.reise.offentligtransport.egenbil.sendebekreftelse.maned"}}
                            {{/hvisLik}}
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    {{/forFaktum}}
{{else}}
    <tr>
        <td class="sjekket"></td>
        <td>
            <span>
                {{hentTekst "soknad.reise.offentligtransport.egenbil.false"}}.
                {{hentTekst "oppsummering.reise.offentligtransport.egenbil.aarsak"}}
            </span>
        </td>
    </tr>
    <tr>
        <td></td>
        <td>
            <table>
                {{#forFaktumHvisSant (concat faktumPrefiks "offentligtransport.egenbil.aarsak.helse")}}
                    <tr>
                        <td class="sjekket"></td>
                        <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.aarsak.helse"}}</td>
                    </tr>
                {{/forFaktumHvisSant}}
                {{#forFaktumHvisSant (concat faktumPrefiks "offentligtransport.egenbil.aarsak.ikkebil")}}
                    <tr>
                        <td class="sjekket"></td>
                        <td>{{hentTekst "soknad.reise.offentligtransport.egenbil.aarsak.ikkebil"}}</td>
                    </tr>
                {{/forFaktumHvisSant}}
                {{#forFaktumHvisSant (concat faktumPrefiks "offentligtransport.egenbil.aarsak.annet")}}
                    <tr>
                        <td class="sjekket"></td>
                        <td>
                            <span>{{hentTekst "soknad.reise.offentligtransport.egenbil.aarsak.annet"}}:</span>
                            {{#forFaktum (concat faktumPrefiks "offentligtransport.egenbil.aarsak.annet.tekst")}}
                                <em>{{value}}</em>
                            {{/forFaktum}}
                        </td>
                    </tr>
                {{/forFaktumHvisSant}}
            </table>
        </td>
    </tr>

    {{#forFaktumHvisSant (concat faktumPrefiks "offentligtransport.drosje")}}
        <tr>
            <td class="sjekket"></td>
            <td><span>{{hentTekst "soknad.reise.offentligtransport.egenbil.drosje.true"}}</span></td>
        </tr>
        {{#forFaktum (concat faktumPrefiks "offentligtransport.drosje.belop")}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{hentTekst "oppsummering.reise.offentligtransport.drosje.belop"}}</span>
                    <span><em>{{value}}</em></span>
                    <span>{{hentTekst "soknad.krdag"}}</span>
                </td>
            </tr>
        {{/forFaktum}}
    {{else}}
        <tr>
            <td class="sjekket"></td>
            <td>
                <span>{{hentTekst "soknad.reise.offentligtransport.egenbil.drosje.false"}}:</span>
                {{#forFaktum (concat faktumPrefiks "offentligtransport.drosje.tekst")}}
                    <span>
                        <em>{{value}}</em>
                    </span>
                {{/forFaktum}}
            </td>
        </tr>
    {{/forFaktumHvisSant}}
{{/forFaktumHvisSant}}