<soknad temaKode="IND" xmlns="urn:no.nav.sbl">
    <!-- Dagpengerside. Brukes kun hvis ARENA er nede -->
    <faktum>
        <id>dagpenger</id>
    </faktum>

    <!-- Informasjonsside -->
    <faktum>
        <id>informasjonsside.dagpenger</id>
    </faktum>
    <faktum>
        <id>informasjonsside.dagpenger.beholde.soketilleggsstonad</id>
    </faktum>
    <faktum>
        <id>informasjonsside.institusjon</id>
    </faktum>
    <faktum>
        <id>informasjonsside.institusjon.ja.hvaslags</id>
    </faktum>
    <faktum>
        <id>informasjonsside.institusjon.ja.hvaslags.annet.soketilleggstonad</id>
    </faktum>
    <faktum>
        <id>informasjonsside.kvalifiseringsprogram</id>
    </faktum>
    <faktum>
        <id>informasjonsside.kvalifiseringsprogram.ja.soketilleggsstonad</id>
    </faktum>
    <faktum>
        <id>informasjonsside.deltarIIntroprogram</id>
    </faktum>
    <faktum>
        <id>informasjonsside.deltarIIntroprogram.info</id>
    </faktum>

    <!-- Tiltaksliste -->
    <faktum>
        <id>tiltaksliste.tiltakFraArena</id>
        <flereTillatt>true</flereTillatt>
    </faktum>
    <faktum>
        <id>tiltaksliste.valgtTiltak</id>
    </faktum>
    <faktum>
        <id>tiltaksliste.annetTiltak</id>
        <dependOn>tiltaksliste.valgtTiltak</dependOn>
        <dependOnValues>
            <value>annetTiltak</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>tiltaksliste.tiltakslistenErTom</id>
    </faktum>

    <!-- Trygd og pensjon -->
    <faktum>
        <id>trygdogpensjon</id>
    </faktum>
    <faktum>
        <id>trygdogpensjon.utbetalere</id>
        <flereTillatt>true</flereTillatt>
    </faktum>

    <!-- Barnetillegg -->
    <faktum>
        <id>barn</id>
        <flereTillatt>true</flereTillatt>
    </faktum>

    <!-- Personalia -->
    <faktum>
        <id>personalia</id>
        <erSystemFaktum>true</erSystemFaktum>
    </faktum>

    <!-- Tilleggsopplysninger -->
    <faktum>
        <id>tilleggsopplysninger.fritekst</id>
    </faktum>

    <!-- Ekstra vedlegg -->
    <faktum>
        <id>ekstraVedlegg</id>
        <flereTillatt>true</flereTillatt>
    </faktum>
    <vedlegg>
        <faktum>ekstraVedlegg</faktum>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>N6</skjemaNummer>
        <flereTillatt>true</flereTillatt>
    </vedlegg>

    <!-- Vedlegg -->
    <vedlegg>
        <skjemaNummer>H8</skjemaNummer>
        <faktum>tiltaksliste.annetTiltak</faktum>
        <onValues>
            <value>PRAKSORD</value>
        </onValues>
    </vedlegg>

    <vedlegg>
        <faktum>trygdogpensjon</faktum>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>K5</skjemaNummer>
        <skjemanummerTillegg>tiltakspenger</skjemanummerTillegg>
    </vedlegg>

    <vedlegg>
        <faktum>barn</faktum>
        <skjemaNummer>X8</skjemaNummer>
        <forSystemfaktum>false</forSystemfaktum>
        <flereTillatt>true</flereTillatt>
    </vedlegg>

</soknad>