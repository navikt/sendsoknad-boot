<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.7.2</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>no.nav.sbl</groupId>
    <artifactId>sendsoknad</artifactId>
    <version>0.0.1</version>
    <packaging>pom</packaging>
    <name>sendsoknad</name>
    <description>Spring boot variant av sendsoknad</description>

    <modules>
        <module>sendsoknad-boot</module>
        <module>sendsoknad-domain</module>
        <module>sendsoknad-mock</module>
        <module>sendsoknad-pdfutility</module>
        <module>sendsoknad-auth</module>
        <module>sendsoknad-innsending-clients</module>
        <module>sendsoknad-innsending</module>
        <module>sendsoknad-consumer</module>
        <module>sendsoknad-business</module>
        <module>sendsoknad-web</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <kotlin.version>1.8.22</kotlin.version>
        <java.version>17</java.version>

        <!-- Tjenestespesifikasjoner -->
        <nav-fim-arbeidsforhold-3.version>1.2019.09.25-00.21-49b69f0625e0</nav-fim-arbeidsforhold-3.version> <!-- was 1.1.1 -->
