<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.7.2</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>no.nav.sbl</groupId>
    <artifactId>sendsoknad</artifactId>
    <version>0.0.1</version>
    <packaging>pom</packaging>
    <name>sendsoknad</name>
    <description>Spring boot variant av sendsoknad</description>

    <modules>
        <module>sendsoknad-boot</module>
        <module>sendsoknad-domain</module>
        <module>sendsoknad-mock</module>
        <module>sendsoknad-pdfutility</module>
        <module>sendsoknad-auth</module>
        <module>sendsoknad-innsending-clients</module>
        <module>sendsoknad-innsending</module>
        <module>sendsoknad-consumer</module>
        <module>sendsoknad-business</module>
        <module>sendsoknad-web</module>
    </modules>

    <properties>
        <java.version>17</java.version>

        <!-- Tjenestespesifikasjoner -->
        <nav-fim-arbeidsforhold-3.version>1.2019.09.25-00.21-49b69f0625e0</nav-fim-arbeidsforhold-3.version> <!-- was 1.1.1 -->
        <nav-fim-organisasjon-v4.versjon>1.2019.09.25-00.21-49b69f0625e0</nav-fim-organisasjon-v4.versjon> <!-- was 1.0.0 -->
        <brukerprofil-tjeneste.version>1.2019.09.25-00.21-49b69f0625e0</brukerprofil-tjeneste.version> <!-- 1.8 -->
        <kodeverk-tjeneste.version>1.2019.09.25-00.21-49b69f0625e0</kodeverk-tjeneste.version> <!-- was 2.0.0 -->
        <nav-fim-person.version>1.2019.09.25-00.21-49b69f0625e0</nav-fim-person.version> <!-- was 1.0.4 -->
        <arena-personinfo.version>1.2019.09.25-00.21-49b69f0625e0</arena-personinfo.version> <!-- was 0.0.1 -->
        <dkif-tjenestespesifikasjon.version>1.2019.09.25-00.21-49b69f0625e0</dkif-tjenestespesifikasjon.version> <!-- 1.2 -->
        <arena-sakOgAktivitet.version>1.2019.09.25-00.21-49b69f0625e0</arena-sakOgAktivitet.version> <!-- was 1.0.11 -->
        <arena-maalgruppe.version>1.2019.09.25-00.21-49b69f0625e0</arena-maalgruppe.version> <!-- was 1.0.2 -->

        <!-- Tredjepart libs -->
        <okhttp3.version>4.10.0</okhttp3.version>
        <nocommons.version>0.13.0</nocommons.version>
        <commons-io.version>2.11.0</commons-io.version>
        <joda-time.version>2.12.2</joda-time.version>
        <org.apache.commons-lang3.version>3.12.0</org.apache.commons-lang3.version>
        <commons-fileupload.version>1.4</commons-fileupload.version>
        <jaxb-impl.version>2.3.6</jaxb-impl.version> <!-- Not upgraded -->
        <!-- Scope test-->
        <org.mockito-core.version>3.12.4</org.mockito-core.version> <!-- Not upgraded -->
        <wiremock.version>2.27.2</wiremock.version>
        <no.digipost.digg.version>0.32</no.digipost.digg.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>${commons-io.version}</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
        </dependency>
        <dependency>
            <groupId>jakarta.xml.bind</groupId>
            <artifactId>jakarta.xml.bind-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.sun.xml.bind</groupId>
            <artifactId>jaxb-impl</artifactId>
            <version>${jaxb-impl.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.hsqldb</groupId>
            <artifactId>hsqldb</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-surefire-plugin</artifactId>
                    <version>2.21.0</version>
                    <configuration>
                        <systemPropertyVariables>
                            <java.awt.headless>true</java.awt.headless>
                        </systemPropertyVariables>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

    <repositories>
        <repository>
            <id>github-package-registry-navikt</id>
            <url>https://maven.pkg.github.com/navikt/maven-release</url>
        </repository>
        <repository>
            <id>jbossrepo</id>
            <url>https://repository.jboss.org/nexus/content/repositories/releases</url>
        </repository>
        <repository>
            <id>jitpack</id>
            <url>https://jitpack.io</url>
        </repository>
    </repositories>
</project>
